{
    "AWSEBDockerrunVersion":2,
    "containerDefinitions":[
       {
        "name":"client",
        "image":"eimearnolan/multi-client",
        "hostname": "client",
        "essential":false
       },
       {
        "name":"server",
        "image":"eimearnolan/multi-server",
        "hostname": "api",
        "essential":false
       } ,
       {
        "name":"worker",
        "image":"eimearnolan/multi-worker",
        "hostname": "worker",
        "essential":false
       },  
       {
        "name":"nginx",
        "image":"eimearnolan/multi-nginx",
        "essential":true,
        "portMappings": [
            {
            "hostPort": 80,
            "containerPort": 80
        }
    ],
    "links": ["client", "server"]
       }
    ]
}